{
  "manifest_version": 2,
  "name": "Formatting Formula",
  "description": "Chrome Extension",
  "version": "1.0",
  "author": "@rsm0128",
  "permissions": [
    "activeTab",
    "http://*/*",
    "https://*/*",
    "file:///*",
    "storage"
  ],
  "content_scripts" : [
    {
      "matches": ["http://*/*", "https://*/*", "file:///*"],
      "js": ["jquery.js", "myscript.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_title": "Formatting",
    "default_popup": "popup.html"
  }
}